// C++ program to find the sum of all elements in an array
#include <iostream>
using namespace std;

int main()
{
    // Initialize the array
    int arr[] = { 1, 2, 3, 4, 5 };

    // size of the array
    int n = sizeof(arr) / sizeof(arr[0]);

    // sum of the array elements
    int sum = 0;
    for (int i = 0; i < n; i++)
        sum += arr[i];

    cout << "Sum: " << sum << endl;

    return 0;
}
